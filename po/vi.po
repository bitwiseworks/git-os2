#, sh-format
msgid "Bad rev input: $arg"
msgstr "Đầu vào rev sai: $arg"

#: git-bisect.sh:281
#, sh-format
msgid "Bad rev input: $bisected_head"
msgstr "Đầu vào rev sai: $bisected_head"

#: git-bisect.sh:290
#, sh-format
msgid "Bad rev input: $rev"
msgstr "Đầu vào rev sai: $rev"

